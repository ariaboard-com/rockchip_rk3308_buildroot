# <file system>			<mount pt>		<type>		<options>		<dump>	<pass>
/dev/root			/			ext4		ro,noatime,nodiratime,noauto		0	1
proc				/proc			proc		defaults		0	0
devpts				/dev/pts		devpts		defaults,gid=5,mode=620	0	0
tmpfs				/dev/shm		tmpfs		mode=0777		0	0
tmpfs				/tmp			tmpfs		mode=1777		0	0
tmpfs				/run			tmpfs		mode=0755,nosuid,nodev	0	0
sysfs				/sys			sysfs		defaults		0	0
debug				/sys/kernel/debug	debugfs		defaults		0	0
pstore				/sys/fs/pstore		pstore		defaults		0	0
/dev/block/by-name/oem		/oem			ext4		ro,defaults,noatime,nodiratime		0	0
/dev/block/by-name/userdata	/userdata		ext4		defaults,noatime,nodiratime		0	2
tmpfs				/var/lib/pulse		tmpfs		mode=0755,nosuid,nodev	0	0
